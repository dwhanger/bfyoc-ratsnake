{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "name": "doc",
      "databaseName": "bfyoc-icecream",
      "collectionName": "ratings",
      "connectionStringSetting": "AzureCosmosDBConnectionString",
      "direction": "in",
      "Id": "{Query.ratingId}",
      "PartitionKey": "{Query.ratingId}"
    }
  ]
  
}